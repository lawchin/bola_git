<Login>:
	name: 'login_page'
	MDBoxLayout:
		orientation: 'vertical'
		MDBoxLayout:
			orientation: 'vertical'
			size_hint: (1,.35)
			FitImage:
				source: 'img/wclogo.png'
				size_hint_y: None  # Tells the layout to ignore the size_hint in y dir
				size_hint_x: None
				size_hint: .5,.4

				pos_hint: {"center_x": 0.5, "center_y": .5}


		MDBoxLayout:
			orientation: 'vertical'
			size_hint: 1,.2

			MDTextField:
				id: user
				hint_text: "username"
				helper_text: ""
				helper_text_mode: "persistent"
				pos_hint: {"center_x": .5}
				size_hint: .4,.2
				font_size:25
				mode: 'rectangle'
			MDLabel:
				size_hint: 1, .1

			MDTextField:
				id: password
				hint_text: "password"
				helper_text: ""
				helper_text_mode: "persistent"
				pos_hint: {"center_x": .5}
				size_hint: .4,.2
				font_size:25
				mode: 'rectangle'
		MDBoxLayout:
			orientation: 'vertical'
			size_hint: 1, .1
			padding: 25

			MDRoundFlatButton:
				text: 'LOGIN'
				font_size: 12
				pos_hint: {'center_x': 0.5}
				on_release: root.log_in()
				size_hint_y: None
				size_hint_x: None
				size_hint: .4, 0.15

		MDBoxLayout:
			size_hint: (1,.35)


		# MDSpinner:
		# 	id: pusing
		# 	size_hint: None, None
		# 	size: dp(25), dp(25)
		# 	#pos_hint: {'x': .475, 'y': .535}
		# 	pos_hint: {'x': 1, 'y': 1}
		# 	active: False
<Signup>:
	name: 'signup_page'
	MDCard:
		size_hint: None, None
		size: 563, 1000
		pos_hint: {"center_x": 0.5, "center_y": 0.5}
		elevation: 10
		padding: 100
		spacing: 20
		orientation: 'vertical'

		FitImage:
			source: 'img/doho_logo3.png'
			size_hint_y: None  # Tells the layout to ignore the size_hint in y dir
			size_hint_x: None
			height: dp(300)  # The fixed height you want
			width: dp(300)
			pos_hint: {"center_x": 0.5, "center_y": .5}


		MDLabel:
			text: "SIGNUP"
			font_size: 40
			halign: 'center'
			size_hint_y: None
			height: self.texture_size[1]
			padding_y: 15

		MDTextField:
			id: su_fname
			hint_text: "First Name"
			mode: "fill"
			fill_color: 1, 0, 0, 1

		MDTextField:
			id: su_lname
			hint_text: "Last Name"
			mode: "fill"
			fill_color: 1, 0, 0, 1

		MDTextField:
			id: su_hp
			hint_text: "Handphone Number"
			mode: "fill"
			fill_color: 1, 0, 0, .4	

		MDTextField:
			id: su_email
			hint_text: "Email"
			mode: "fill"
			fill_color: 1, 0, 0, .4	

		MDTextField:
			id: su_pasw1
			hint_text: "Password"
			mode: "fill"
			fill_color: 1, 0, 0, .4	

		MDTextField:
			id: su_pasw2
			hint_text: "Re-Type Password"
			mode: "fill"
			fill_color: 1, 0, 0, .4
		MDBoxLayout:
			MDLabel:
				id: submit_error
				text: ''
				color: 1,0,0,1

		MDRoundFlatButton:
			text: 'SUBMIT'
			font_size: 12
			pos_hint: {'center_x': 0.5}
			on_press: root.submit()

		MDRoundFlatButton:
			text: 'CLEAR'
			font_size: 12
			pos_hint: {'center_x': 0.5}
			on_press: root.clear_form()

		MDRoundFlatButton:
			text: 'BACK'
			font_size: 12
			pos_hint: {'center_x': 0.5}
			on_press: 
				root.clear_form()
				app.root.current = "login_page"
				root.manager.transition.direction = "right"


		# Widget:
		# 	size_hint_y: None
		# 	height: 25
